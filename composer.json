{
    "name": "webeweb/jquery-datatables-bundle",
    "description": "Integrate Datatables with Symfony 2 and more",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "webeweb",
        "symfony",
        "bundle",
        "datatables"
    ],
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "homepage": "https://github.com/webeweb/jquery-datatables-bundle",
    "support": {
        "issues": "https://github.com/webeweb/jquery-datatables-bundle/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "doctrine/orm": "^2.5.13",
        "matomo/device-detector": "^3.0|^4.0|^5.0",
        "symfony/dependency-injection": "^4.0|^5.0|^6.0",
        "symfony/event-dispatcher": "^4.0|^5.0|^6.0",
        "symfony/http-foundation": "^4.0|^5.0|^6.0",
        "symfony/http-kernel": "^4.0|^5.0|^6.0",
        "symfony/mailer": "^4.0|^5.0|^6.0",
        "symfony/property-access": "^4.0|^5.0|^6.0",
        "symfony/routing": "^4.0|^5.0|^6.0",
        "symfony/security-core": "^4.4|^5.0|^6.0",
        "symfony/serializer": "^4.4|^5.0|^6.0",
        "symfony/translation": "^4.0|^5.0|^6.0",
        "twig/twig": "^2.10|^3.0",
        "webeweb/core-library": "^8.70"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\": "lib/bootstrap",
            "WBW\\Bundle\\CommonBundle\\": "lib/common",
            "WBW\\Bundle\\DataTablesBundle\\": "src"
        },
        "exclude-from-classmap": [
            "/lib/bootstrap/Tests/",
            "/lib/common/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\BootstrapBundle\\Tests\\": "lib/bootstrap/Tests",
            "WBW\\Bundle\\CommonBundle\\Tests\\": "lib/common/Tests",
            "WBW\\Bundle\\DataTablesBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    }
}
